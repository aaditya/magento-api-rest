{
  "name": "magento-api-rest",
  "version": "2.0.5",
  "description": "A NodeJS wrapper to work with Magento REST APIs.",
  "main": "index",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts",
    "/lib/parser.js",
    "/lib/transform.js"
  ],
  "scripts": {
